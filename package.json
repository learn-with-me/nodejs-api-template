{
  "name": "template-nodejs",
  "version": "1.0.0",
  "description": "Library template",
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prettier": "prettier '**/*.{js,ts}' --ignore-path ./.prettierignore",
    "types": "tsc --noEmit",
    "lint": "eslint '*/**/*.{js,ts}'",
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "rimraf dist",
    "build:lib": "rollup -c",
    "finish": "npm run lint && npm run types",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goel4ever/template-nodejs.git"
  },
  "browserslist": [
    "last 2 version "
  ],
  "author": "goel4ever",
  "license": "MIT",
  "keywords": [
    "template",
    "javascript",
    "package"
  ],
  "bugs": {
    "url": "https://github.com/goel4ever/template-nodejs/issues"
  },
  "homepage": "https://github.com/goel4ever/template-nodejs#readme",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "prettier": "^2.7.1",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "^4.9.3"
  }
}
